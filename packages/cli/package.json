{
  "name": "@node-flow/cli",
  "version": "0.0.1-alpha.10",
  "description": "oclif example Hello World CLI",
  "author": "wayne @WayneGongCN",
  "bin": {
    "nf": "./bin/run"
  },
  "homepage": "https://github.com/WayneGongCN/hello-world",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "WayneGongCN/hello-world",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "dependencies": {
    "@node-flow/core": "^0.0.1-alpha.10",
    "@oclif/core": "^1",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-plugins": "^2.0.1"
  },
  "devDependencies": {
    "@oclif/test": "^2",
    "@types/chai": "^4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.9.4",
    "chai": "^4",
    "globby": "^11",
    "mocha": "^9",
    "oclif": "^3",
    "shx": "^0.3.3",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1"
  },
  "oclif": {
    "bin": "nf",
    "dirname": "nf",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {
      "hello": {
        "description": "Say hello to the world and others"
      }
    }
  },
  "scripts": {
    "lint": "eslint src test",
    "build": "shx rm -rf dist && tsc",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "echo done",
    "posttest:back": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "echo done",
    "test:back": "mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/WayneGongCN/hello-world/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
